<Window x:Class="C2CClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:vm ="clr-namespace:C2CClient.MMViewModels"
        xmlns:local="clr-namespace:C2CClient"
        mc:Ignorable="d"
        Title="Motorway" Height="450" Width="800">
    <Window.DataContext>
        <vm:MotorwayMainViewModel/>
    </Window.DataContext>
    <Grid>
        <UserControl Content="{Binding MMControlPage,Mode=TwoWay}"/>
    </Grid>
</Window>
